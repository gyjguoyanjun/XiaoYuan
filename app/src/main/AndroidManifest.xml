<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tianjieyu.xiaoyuan">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="design_width"
            android:value="768" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />

        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FriendActivity" />
        <activity android:name=".activities.Binding_music_Activity" />
        <activity android:name=".activities.Wangyi_login_Activity"/>
        <activity android:name=".activities.Wangyi_login_spec_Activity" />
<activity android:name=".activities.XuanZeTouXangActivity"/>
        <activity android:name=".activities.MyFollowActivity"/>
        <activity android:name=".activities.PresonSettingActivity"></activity>
        <activity android:name=".activities.LiaoYiXiaActivity"/>
    </application>

</manifest>